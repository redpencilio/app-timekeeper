{
  "version": "0.1",
  "prefixes": {
    "adms": "http://www.w3.org/ns/adms#",
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "mu": "http://mu.semte.ch/vocabularies/core/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "wf": "http://www.w3.org/2005/01/wf/flow#",
    "cal": "http://www.w3.org/2002/12/cal/ical#",
    "prov": "http://www.w3.org/ns/prov#",
    "dct": "http://purl.org/dc/terms/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "ui": "http://www.w3.org/ns/ui#",
    "time": "http://www.w3.org/2006/time#"
  },
  "resources": {
    "customers": {
      "name": "customer",
      "class": "prov:Organization",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "rdfs:label"
        },
        "color": {
          "type": "string",
          "predicate": "ui:color"
        }
      },
      "relationships": {
        "projects": {
          "predicate": "prov:wasAttributedTo",
          "inverse": true,
          "target": "task",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://timekeeper.redpencil.io/customers/"
    },
    "tasks": {
      "name": "task",
      "class": "wf:Task",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "rdfs:label"
        },
        "color": {
          "type": "string",
          "predicate": "ui:color"
        }
      },
      "relationships": {
        "concept-scheme": {
          "predicate": "skos:inScheme",
          "target": "workspace",
          "cardinality": "one"
        },
        "workspace": {
          "predicate": "skos:topConceptOf",
          "target": "workspace",
          "cardinality": "one"
        },
        "parent": {
          "predicate": "skos:broader",
          "target": "task",
          "cardinality": "one"
        },
        "children": {
          "predicate": "skos:broader",
          "inverse": true,
          "target": "task",
          "cardinality": "many"
        },
        "work-logs": {
          "predicate": "dct:subject",
          "inverse": true,
          "target": "work-log",
          "cardinality": "many"
        },
        "customer": {
          "predicate": "prov:wasAttributedTo",
          "target": "customer",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://timekeeper.redpencil.io/tasks/"
    },
    "workspaces": {
      "name": "workspace",
      "class": "skos:ConceptScheme",
      "attributes": {
        "name": {
          "predicate": "rdfs:label",
          "type": "string"
        }
      },
      "relationships": {
        "tasks": {
          "predicate": "skos:inScheme",
          "inverse": true,
          "target": "task",
          "cardinality": "many"
        },
        "projects": {
          "predicate": "skos:topConceptOf",
          "inverse": true,
          "target": "task",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://timekeeper.redpencil.io/workspaces/"
    },
    "work-logs": {
      "name": "work-log",
      "class": "cal:Vevent",
      "attributes": {
        "duration": {
          "type": "string",
          "predicate": "cal:duration"
        },
        "date": {
          "type": "date",
          "predicate": "cal:dtstart"
        }
      },
      "relationships": {
        "task": {
          "predicate": "dct:subject",
          "target": "task",
          "cardinality": "one"
        },
        "person": {
          "predicate": "prov:wasAssociatedWith",
          "target": "person",
          "cardinality": "one"
        },
        "timesheet": {
          "predicate": "skos:member",
          "target": "timesheet",
          "cardinality": "one",
          "inverse": "true"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://timekeeper.redpencil.io/work-logs/"
    },
    "timesheets": {
      "name": "timesheet",
      "class": "skos:Collection",
      "_additionalClasses": "time:Interval",
      "attributes": {
        "start": {
          "type": "date",
          "predicate": "time:hasBeginning"
        },
        "end": {
          "type": "date",
          "predicate": "time:hasEnd"
        },
        "status": {
          "type": "url",
          "predicate": "adms:status"
        }
      },
      "relationships": {
        "work-logs": {
          "predicate": "skos:member",
          "target": "work-log",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://timekeeper.redpencil.io/timesheets/"
    }
  }
}
